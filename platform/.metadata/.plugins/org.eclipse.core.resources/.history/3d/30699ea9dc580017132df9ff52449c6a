	LED_BLUE	=2}LEDS;

struct{

		float	Lat;
		float	Lng;
		float	Acc[3];
		float 	Inc[3];
		float	Speed ;
		float	Level;
		char	date[20];
	}


ResultExec onAnalog(DataFrame* cmd);
ResultExec onLED(DataFrame* cmd);
ResultExec onPWM(DataFrame* cmd);
ResultExec onTouch(DataFrame* cmd);
ResultExec onAccel(DataFrame* cmd);
